{
  "name": "karta_catalog",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": true,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "string",
      "required": true
    },
    "node": {
      "type": "object",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "kartaNode": {
      "type": "belongsTo",
      "model": "karta_node",
      "foreignKey": "nodeId",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "karta": {
      "type": "belongsTo",
      "model": "karta",
      "foreignKey": "kartaId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
