{
  "name": "karta_phase",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mongodb": {
      "collection": "karta_phase",
      "allowExtendedOperators": true
    }
  },
  "mixins": {
    "TimeStamp": {
      "required": true,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "global_name": {
      "type": "string"
    },
    "is_global": {
      "type": "boolean",
      "default": false
    },
    "is_deleted": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "phase": {
      "type": "belongsTo",
      "model": "karta_phase",
      "foreignKey": "phaseId",
      "options": {
        "nestRemoting": true
      }
    },
    "parent": {
      "type": "belongsTo",
      "model": "karta_phase",
      "foreignKey": "parentId",
      "options": {
        "nestRemoting": true
      }
    },
    "karta": {
      "type": "belongsTo",
      "model": "karta",
      "foreignKey": "kartaId",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
