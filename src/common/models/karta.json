{
  "name": "karta",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required" : true,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "target": {
      "type": "number"
    },
    "due_date": {
      "type": "date"
    },
    "version": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "completion_percent": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
