{
  "name": "karta",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": true,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "target": {
      "type": "number"
    },
    "due_date": {
      "type": "date"
    },
    "version": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "completion_percent": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    },
    "node": {
      "type": "hasOne",
      "model": "karta_node",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
