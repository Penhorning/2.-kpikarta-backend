{
  "name": "karta-history",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mongodb": {
      "collection": "karta-history",
      "allowExtendedOperators": true
    }
  },
  "mixins": {
    "TimeStamp": {
      "required": true,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "event": {
      "type": "string",
      "required": true
    },
    "event_key": {
      "type": "string",
      "required": true
    },
    "event_value": {
      "type": "string",
      "required": true
    },
    "content_id": {
      "type": "string",
      "required": true
    },
    "version": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
